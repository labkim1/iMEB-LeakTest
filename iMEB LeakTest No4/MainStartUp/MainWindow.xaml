<Controls:MetroWindow
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:ni="http://schemas.ni.com/controls/2009/xaml/presentation"
        xmlns:Controls="clr-namespace:MahApps.Metro.Controls;assembly=MahApps.Metro"
        xmlns:notifications="clr-namespace:Notifications.Wpf.Controls;assembly=Notifications.Wpf"                         
        xmlns:TLog="clr-namespace:TLog"
        xmlns:iconPacks="http://metro.mahapps.com/winfx/xaml/iconpacks" 
        xmlns:System="clr-namespace:System;assembly=mscorlib" x:Class="iMEB_LeakTest_No4.MainWindow"
        ShowIconOnTitleBar="True" ShowTitleBar="True" ShowDialogsOverTitleBar="True"                      
        Title="iMEB Leak Test" Height="1024" Width="1280" Loaded="Window_Loaded" Icon="/iMEB LeakTest No4;component/iMEB.ico" Closing="MetroWindow_Closing" >

    <Controls:MetroWindow.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Resources/Icons.xaml" />
            </ResourceDictionary.MergedDictionaries>
            <TLog:NullVisibilityConverter x:Key="NullVisibilityConverter" />
            <TLog:VisibilityBoolConverter x:Key="VisibilityBoolConverter" />
            <TLog:FreezeTextConverter     x:Key="FreezeTextConverter" />
            <TLog:NullBooleanConverter    x:Key="NullBooleanConverter" />
        </ResourceDictionary>

    </Controls:MetroWindow.Resources>

    <Controls:MetroWindow.RightWindowCommands>
        <Controls:WindowCommands>
            <Button x:Name="SystemConfig" Content="관리자" Click="SystemConfig_Click" />
        </Controls:WindowCommands>
    </Controls:MetroWindow.RightWindowCommands>
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="220" />
            <ColumnDefinition Width="*" />
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="130"/>
            <RowDefinition Height="1*"/>
            <RowDefinition Height="1*"/>
            <RowDefinition Height="AUTO"/>
            <RowDefinition Height="32"/>
        </Grid.RowDefinitions>
        <notifications:NotificationArea x:Name="WindowArea" Position="TopLeft" MaxItems="5"/>
        <Popup x:Name="NGPopup" Grid.Column="0" Grid.ColumnSpan="1" Grid.Row="1" Grid.RowSpan="3" 
                Placement="Center" AllowsTransparency="True" StaysOpen="False" PlacementTarget="{Binding RelativeSource={RelativeSource AncestorType=Window}}">
            <Grid>
                <Border Width="450" Height="300" BorderBrush="Blue" BorderThickness="1" Background="Honeydew" >
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="60" />
                            <ColumnDefinition Width="*" />
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="60"/>
                            <RowDefinition Height="*"/>
                            <RowDefinition Height="30"/>
                        </Grid.RowDefinitions>
                        <Image Grid.Column="0" Grid.Row="0" Source="pack://siteoforigin:,,,/Resources/Warning.png"   />
                        <Label Grid.Column="1" Grid.Row="0" FontSize="24" Foreground="Red" Margin="20,0,0,0" VerticalAlignment="Center" FontWeight="Bold" FontStyle="Italic">NG 및 알람 정보 표시</Label>
                        <ScrollViewer Grid.Column="0" Grid.ColumnSpan="2" Grid.Row="1" Margin="2,2,2,2" >
                            <StackPanel Orientation="Vertical" Margin="3,3,3,3" >
                                <TextBlock  x:Name="NGMessage1" FontSize="16" TextWrapping="Wrap"></TextBlock>
                            </StackPanel>
                        </ScrollViewer>
                        <Label Grid.Column="0" Grid.ColumnSpan="2" Grid.Row="2">본 화면은 자동으로 사라집니다. 자동생산, 수동변경시 ...</Label>
                    </Grid>
                </Border>
            </Grid>
        </Popup>
        <Popup x:Name="OKPopup" Grid.Column="0" Grid.ColumnSpan="1" Grid.Row="1" Grid.RowSpan="3" Placement="Center" AllowsTransparency="True">
            <Grid>
                <Border Width="600" Height="300" BorderBrush="Blue" BorderThickness="1" Background="Honeydew" >
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="60" />
                            <ColumnDefinition Width="*" />
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="60"/>
                            <RowDefinition Height="*"/>
                            <RowDefinition Height="30"/>
                        </Grid.RowDefinitions>
                        <Image Grid.Column="0" Grid.Row="0"  />
                        <Label Grid.Row="0" Foreground="Blue" FontSize="40" Margin="4,0,76,0" VerticalAlignment="Center" FontWeight="Bold" FontStyle="Italic" Grid.ColumnSpan="2">제품 OK</Label>
                        <ScrollViewer Grid.Column="0" Grid.ColumnSpan="2" Grid.Row="1" Margin="2,2,2,2" >
                            <StackPanel Orientation="Vertical" >
                                <TextBlock Margin="10,20,10,10" x:Name="OKMessage1" FontSize="25" ></TextBlock>
                            </StackPanel>
                        </ScrollViewer>

                    </Grid>
                </Border>
                <Button x:Name="okpopup_close" Content="X" VerticalAlignment="Top" Margin="572,4,5,0" Click="okpopup_close_Click"/>
            </Grid>
        </Popup>
        <!-- 왼쪽 상단 로그 및 이미지-->
        <Grid Grid.Column="0" Grid.Row="0"  >
            <Grid.RowDefinitions>
                <RowDefinition Height="*"/>
                <RowDefinition Height="40"/>
                <RowDefinition Height="30"/>
            </Grid.RowDefinitions>
            <Border Grid.Row="0" Grid.RowSpan="2"  BorderThickness="1" Margin="1,1,1,1">
                <Border.BorderBrush>
                    <SolidColorBrush Color="{DynamicResource {x:Static SystemColors.GradientActiveCaptionColorKey}}"/>
                </Border.BorderBrush>
                <Image x:Name="Title_Image1" Loaded="Title_Image_Loaded" Source="pack://siteoforigin:,,,/Resources/ECUAssy.png"  />
            </Border>
            <Border Grid.Row="3"  BorderThickness="1" Margin="1,1,1,1">
                <Border.BorderBrush>
                    <SolidColorBrush Color="{DynamicResource {x:Static SystemColors.GradientActiveCaptionColorKey}}"/>
                </Border.BorderBrush>
                <Image x:Name="Title_Image2" Loaded="Title_Image_Loaded" Source="pack://siteoforigin:,,,/Resources/colorTone_img1.jpg" />
            </Border>
        </Grid>
        <!-- 오른쪽 상단 타이늘 및 정보 표시-->
        <Grid Grid.Column="1" Grid.Row="0">
            <Grid.RowDefinitions>
                <RowDefinition Height="50"/>
                <RowDefinition Height="35"/>
                <RowDefinition Height="*"/>
            </Grid.RowDefinitions>
            <Grid Grid.Row="0">
                <Grid.Triggers>
                    <EventTrigger RoutedEvent="Grid.Loaded">
                        <BeginStoryboard>
                            <Storyboard>
                                <DoubleAnimation From="1" To="10" Duration="00:00:03" AutoReverse="True" RepeatBehavior="Forever"
                                                    Storyboard.TargetName="blurEffect" 
                                                    Storyboard.TargetProperty="Radius">
                                </DoubleAnimation>
                            </Storyboard>
                        </BeginStoryboard>
                    </EventTrigger>
                </Grid.Triggers>
                <Border >
                    <Border.Background>
                        <SolidColorBrush Color="{DynamicResource {x:Static SystemColors.HotTrackColorKey}}"/>
                    </Border.Background>
                    <Border.BorderBrush>
                        <SolidColorBrush Color="{DynamicResource {x:Static SystemColors.GradientActiveCaptionColorKey}}"/>
                    </Border.BorderBrush>
                    <StackPanel Orientation="Horizontal" VerticalAlignment="Center" HorizontalAlignment="Right">
                        <Label x:Name="MAObject" Content="iMEB ASSY LEAK EOL TEST Ver " FontSize="20" FontFamily="Myanmar Text" FontWeight="Bold" VerticalAlignment="Center" VerticalContentAlignment="Center" Foreground="#FFE2E1E1">
                            <Label.Effect>
                                <BlurEffect Radius="1" x:Name="blurEffect" RenderingBias="Quality"/>
                            </Label.Effect>
                        </Label>
                        <Label x:Name="lbl_ProgramVersion" Content="0.000"  FontSize="20" FontFamily="Myanmar Text" FontWeight="Bold" VerticalAlignment="Center" VerticalContentAlignment="Center" Foreground="#FFE2E1E1"/>
                        <Button x:Name="TouchKEY" Click="TouchKEY_Click" Content="KEY"/>
                    </StackPanel>
                </Border>
            </Grid>
            <Grid Grid.Row="1">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="0.6*" />
                    <ColumnDefinition Width="1*" />
                    <ColumnDefinition Width="0.7*" />
                    <ColumnDefinition Width="2*" />
                </Grid.ColumnDefinitions>
                <Border Grid.Column="0" BorderThickness="1">
                    <Border.BorderBrush>
                        <SolidColorBrush Color="{DynamicResource {x:Static SystemColors.GradientActiveCaptionColorKey}}"/>
                    </Border.BorderBrush>
                    <StackPanel Orientation="Horizontal" VerticalAlignment="Center">
                        <iconPacks:PackIconMaterial Margin="5,0,0,0" Kind="EarthBox" Foreground="Black" VerticalAlignment="Center" HorizontalAlignment="Left" />
                        <Label Content="PRODUCT" VerticalContentAlignment="Center"/>
                        <Label x:Name="D_ProductCode" Content="H1" Width="35" VerticalContentAlignment="Center" HorizontalContentAlignment="Right" VerticalAlignment="Stretch" Foreground="Blue" FontSize="14" FontWeight="Bold"/>
                    </StackPanel>
                </Border>
                <Border Grid.Column="1" BorderThickness="1" Margin="1,0,1,0">
                    <Border.BorderBrush>
                        <SolidColorBrush Color="{DynamicResource {x:Static SystemColors.GradientActiveCaptionColorKey}}"/>
                    </Border.BorderBrush>
                    <StackPanel Orientation="Horizontal" VerticalAlignment="Center">
                        <iconPacks:PackIconMaterial Margin="5,0,0,0"  Kind="Barcode" VerticalAlignment="Center" HorizontalAlignment="Left"/>
                        <Label Content="BARCODE"/>
                        <Label x:Name="D_BarCode" Content="01234567891" Width="130"  VerticalContentAlignment="Center" HorizontalContentAlignment="Right" VerticalAlignment="Stretch" Foreground="Blue" FontSize="14" FontWeight="Bold"/>
                    </StackPanel>
                </Border>
                <Border Grid.Column="2" BorderThickness="1"  Margin="0,0,1,0">
                    <Border.BorderBrush>
                        <SolidColorBrush Color="{DynamicResource {x:Static SystemColors.GradientActiveCaptionColorKey}}"/>
                    </Border.BorderBrush>
                    <StackPanel Orientation="Horizontal" VerticalAlignment="Center">
                        <iconPacks:PackIconMaterial Margin="5,0,0,0"  Kind="Target" VerticalAlignment="Center" HorizontalAlignment="Left"/>
                        <Label Content="USER"/>
                        <Label x:Name="D_User" Content="User" Width="100"  VerticalContentAlignment="Center" HorizontalContentAlignment="Right" VerticalAlignment="Stretch" Foreground="Blue" FontSize="14" FontWeight="Bold"/>
                    </StackPanel>
                </Border>
                <Border Grid.Column="3" BorderThickness="1"  Margin="0,0,1,0">
                    <Border.BorderBrush>
                        <SolidColorBrush Color="{DynamicResource {x:Static SystemColors.GradientActiveCaptionColorKey}}"/>
                    </Border.BorderBrush>
                    <StackPanel Orientation="Horizontal" VerticalAlignment="Center">
                        <iconPacks:PackIconMaterial Margin="5,0,0,0"  Kind="PackageUp" VerticalAlignment="Center" HorizontalAlignment="Left"/>
                        <Label Content="MES "/>
                        <Label x:Name="D_MES" Content="message" Width="420"  VerticalContentAlignment="Center" HorizontalContentAlignment="Left" VerticalAlignment="Stretch" Foreground="Gray" FontSize="12" FontWeight="Bold"/>
                    </StackPanel>
                </Border>
            </Grid>
            <Grid Grid.Row="2">
                <Border  BorderThickness="1" Margin="0,-1,0,1">
                    <Border.BorderBrush>
                        <SolidColorBrush Color="{DynamicResource {x:Static SystemColors.GradientActiveCaptionColorKey}}"/>
                    </Border.BorderBrush>

                    <StackPanel Orientation="Horizontal">
                        <Border x:Name="Test_Lamp_0" BorderThickness="1" Margin="3,3,3,3" Width="120" MouseLeftButtonDown="Test_Lamp_MouseLeftButtonDown">
                            <Border.BorderBrush>
                                <SolidColorBrush Color="{DynamicResource {x:Static SystemColors.ActiveCaptionColorKey}}"/>
                            </Border.BorderBrush>
                            <StackPanel Orientation="Horizontal" VerticalAlignment="Center">
                                <iconPacks:PackIconMaterial Margin="5,0,0,0" Kind="Adjust" VerticalAlignment="Center" HorizontalAlignment="Left"/>
                                <Label Content="외부 LEAK" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" />
                            </StackPanel>
                        </Border>
                        <Border x:Name="Test_Lamp_1" BorderThickness="1" Margin="3,3,3,3" Width="120" MouseLeftButtonDown="Test_Lamp_MouseLeftButtonDown" >
                            <Border.BorderBrush>
                                <SolidColorBrush Color="{DynamicResource {x:Static SystemColors.ActiveCaptionColorKey}}"/>
                            </Border.BorderBrush>
                            <StackPanel Orientation="Horizontal" VerticalAlignment="Center">
                                <iconPacks:PackIconMaterial Margin="5,0,0,0" Kind="Adjust" VerticalAlignment="Center" HorizontalAlignment="Left"/>
                                <Label Content="-----------------" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" />
                            </StackPanel>
                        </Border>
                        <Border x:Name="Test_Lamp_1_1"  BorderThickness="1" Margin="3,3,3,3" Width="100" MouseLeftButtonDown="Test_Lamp_MouseLeftButtonDown">
                            <Border.BorderBrush>
                                <SolidColorBrush Color="{DynamicResource {x:Static SystemColors.ActiveCaptionColorKey}}"/>
                            </Border.BorderBrush>
                            <Label Content="내부 LEAK(5.0)" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" />
                        </Border>
                        <Border x:Name="Test_Lamp_3" BorderThickness="1" Margin="3,3,3,3" Width="160" MouseLeftButtonDown="Test_Lamp_MouseLeftButtonDown">
                            <Border.BorderBrush>
                                <SolidColorBrush Color="{DynamicResource {x:Static SystemColors.ActiveCaptionColorKey}}"/>
                            </Border.BorderBrush>
                            <StackPanel Orientation="Horizontal" VerticalAlignment="Center">
                                <iconPacks:PackIconMaterial Margin="5,0,0,0" Kind="CarConnected" VerticalAlignment="Center" HorizontalAlignment="Left"/>
                                <Label Content="ECU LEAK TEST" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" />
                            </StackPanel>
                        </Border>
                        <Border x:Name="Test_Analay1" BorderThickness="1" Margin="3,3,3,3" Width="120" MouseLeftButtonDown="Test_Lamp_MouseLeftButtonDown">
                            <Border.BorderBrush>
                                <SolidColorBrush Color="{DynamicResource {x:Static SystemColors.ActiveCaptionColorKey}}"/>
                            </Border.BorderBrush>
                            <StackPanel Orientation="Horizontal" VerticalAlignment="Center">
                                <iconPacks:PackIconMaterial Margin="5,0,0,0" Kind="CarConnected" VerticalAlignment="Center" HorizontalAlignment="Left"/>
                                <Label Content="모터전류평가" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" />
                            </StackPanel>
                        </Border>
                        <Border x:Name="Test_Lamp_2" BorderThickness="1" Margin="3,3,3,3" Width="120" MouseLeftButtonDown="Test_Lamp_MouseLeftButtonDown">
                            <Border.BorderBrush>
                                <SolidColorBrush Color="{DynamicResource {x:Static SystemColors.ActiveCaptionColorKey}}"/>
                            </Border.BorderBrush>
                            <StackPanel Orientation="Horizontal" VerticalAlignment="Center">
                                <iconPacks:PackIconMaterial Margin="5,0,0,0" Kind="CarConnected" VerticalAlignment="Center" HorizontalAlignment="Left"/>
                                <Label Content="ECU DTC" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" />
                            </StackPanel>
                        </Border>
                        <Border x:Name="Test_Analay" BorderThickness="1" Margin="3,3,3,3" Width="120" MouseLeftButtonDown="Test_Lamp_MouseLeftButtonDown">
                            <Border.BorderBrush>
                                <SolidColorBrush Color="{DynamicResource {x:Static SystemColors.ActiveCaptionColorKey}}"/>
                            </Border.BorderBrush>
                            <StackPanel Orientation="Horizontal" VerticalAlignment="Center">
                                <iconPacks:PackIconMaterial Margin="5,0,0,0" Kind="Delta" VerticalAlignment="Center" HorizontalAlignment="Left"/>
                                <Label Content="ANALYSIS" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" />
                            </StackPanel>
                        </Border>

                        <Border Visibility="Hidden" x:Name="Test_Lamp_3_1" BorderThickness="1" Margin="3,3,3,3" Width="140" MouseLeftButtonDown="Test_Lamp_MouseLeftButtonDown">
                            <Border.BorderBrush>
                                <SolidColorBrush Color="{DynamicResource {x:Static SystemColors.ActiveCaptionColorKey}}"/>
                            </Border.BorderBrush>
                            <Label Content="ECU LEAK TEST(MC34)" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" />
                        </Border>
                    </StackPanel>
                </Border>
            </Grid>
        </Grid>
        <!-- 왼쪽 중앙 메인 메뉴-->
        <Grid Grid.Column="0" Grid.Row="1" Grid.RowSpan="3">
            <GroupBox Header="Menu">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="*"/>
                        <RowDefinition Height="270"/>
                        <RowDefinition Height="50"/>
                    </Grid.RowDefinitions>
                    <StackPanel Grid.Row="0" Orientation="Vertical">
                        <Button Margin="3,3,3,3" x:Name="Btn_USER" Click="Btn_USER_Click"  >
                            <StackPanel Orientation="Horizontal">
                                <iconPacks:PackIconMaterial Kind="Account" VerticalAlignment="Center" HorizontalAlignment="Left"/>
                                <Label Content="USER" Width="120" HorizontalContentAlignment="Center" FontSize="14" />
                            </StackPanel>
                        </Button>
                        <Button Margin="3,3,3,3" x:Name="Btn_OPEN">
                            <StackPanel Orientation="Horizontal">
                                <iconPacks:PackIconMaterial Kind="OpenInApp" VerticalAlignment="Center" HorizontalAlignment="Left"/>
                                <Label Content="OPEN" Width="120" HorizontalContentAlignment="Center" FontSize="14" />
                            </StackPanel>
                        </Button>
                        <Button Margin="3,3,3,3" x:Name="Btn_EDIT">
                            <StackPanel Orientation="Horizontal">
                                <iconPacks:PackIconMaterial Kind="Textbox" VerticalAlignment="Center" HorizontalAlignment="Left"/>
                                <Label Content="EDIT" Width="120" HorizontalContentAlignment="Center" FontSize="14" />
                            </StackPanel>
                        </Button>
                        <Button Margin="3,3,3,3" x:Name="Btn_TEST">
                            <StackPanel Orientation="Horizontal">
                                <iconPacks:PackIconMaterial Kind="Run" VerticalAlignment="Center" HorizontalAlignment="Left"/>
                                <Label Content="TEST" Width="120" HorizontalContentAlignment="Center" FontSize="14" />
                            </StackPanel>
                        </Button>
                        <Button Margin="3,3,3,3" x:Name="Btn_RESULT" Click="Btn_RESULT_Click" >
                            <StackPanel Orientation="Horizontal">
                                <iconPacks:PackIconMaterial Kind="ChartLine" VerticalAlignment="Center" HorizontalAlignment="Left"/>
                                <Label Content="RESULT" Width="120" HorizontalContentAlignment="Center" FontSize="14" />
                            </StackPanel>
                        </Button>
                        <Button Margin="3,3,3,3" x:Name="Btn_Log" Click="Btn_Log_Click">
                            <StackPanel Orientation="Horizontal">
                                <iconPacks:PackIconMaterial Kind="History" VerticalAlignment="Center" HorizontalAlignment="Left"/>
                                <Label Content="LOG" Width="120" HorizontalContentAlignment="Center" FontSize="14" />
                            </StackPanel>
                        </Button>
                        <Button Margin="3,3,3,3" x:Name="Btn_SELFTEST">
                            <StackPanel Orientation="Horizontal">
                                <iconPacks:PackIconMaterial Kind="Check" VerticalAlignment="Center" HorizontalAlignment="Left"/>
                                <Label Content="SELF TEST" Width="120" HorizontalContentAlignment="Center" FontSize="14" />
                            </StackPanel>
                        </Button>
                    </StackPanel>
                    <StackPanel Grid.Row="1" Orientation="Vertical">
                        <Border>
                            <ProgressBar x:Name="Pb_Motor" VerticalAlignment="Top" Value="0"  Height="5"  Margin="2,0,2,0" Foreground="Red" SmallChange="0.01" />
                        </Border>
                        <Border>
                            <StackPanel Orientation="Horizontal">
                                <Label x:Name="MBC_Pos" Foreground="Green" Content=""/>
                                <Label x:Name="MBC_Amp" Foreground="Green" Content=""/>
                                <Label x:Name="MBC_ReadCheck" Foreground="Green" Content=""/>
                                <Label x:Name="MBC_StopWatchTime" Foreground="Green" Content=""/>
                            </StackPanel>

                        </Border>
                        <Border>
                            <StackPanel Orientation="Horizontal">
                                <Label Width="70" Content="PSU"/>
                                <Label Width="80" HorizontalContentAlignment="Right" x:Name="L_CH0" Content="0.0"/>
                                <Label Content="V"/>
                            </StackPanel>
                        </Border>
                        <Border>
                            <StackPanel Orientation="Horizontal">
                                <Label Width="70" Content="PSU Amp"/>
                                <Label Width="80" HorizontalContentAlignment="Right" x:Name="L_CH1" Content="0.0"/>
                                <Label Content="A"/>
                            </StackPanel>
                        </Border>
                        <Border>
                            <StackPanel Orientation="Horizontal">
                                <Label Width="50" Content="MC1,2"/>
                                <Label Width="80" HorizontalContentAlignment="Right" x:Name="L_VAC1" Content="0.0"/>
                                <Label Content="mmHg"/>
                            </StackPanel>
                        </Border>
                        <Border>
                            <StackPanel Orientation="Horizontal">
                                <Label Width="50" Content="MC3,4"/>
                                <Label Width="80" HorizontalContentAlignment="Right" x:Name="L_VAC2" Content="0.0"/>
                                <Label Content="mmHg"/>
                            </StackPanel>
                        </Border>
                        <Label Content="CPU" />
                        <ProgressBar x:Name="Pb_CPU"  Height="6"  Margin="2,0,2,0"/>
                        <Label Content="MEMORY" />
                        <ProgressBar x:Name="Pb_MEMORY"  Height="6"  Margin="2,0,2,0"/>
                        <Label Content="HDD" />
                        <ProgressBar x:Name="Pb_HDD"  Height="6"  Margin="2,0,2,0"/>
                    </StackPanel>
                    <StackPanel Grid.Row="2" Orientation="Vertical">
                        <Button Margin="3,3,3,3" x:Name="Btn_QUIT" Click="Btn_QUIT_Click" >
                            <StackPanel Orientation="Horizontal">
                                <iconPacks:PackIconMaterial Kind="Power" VerticalAlignment="Center" HorizontalAlignment="Left"/>
                                <Label Content="QUIT" Width="120" HorizontalContentAlignment="Center" FontSize="14" />
                            </StackPanel>
                        </Button>
                    </StackPanel>
                </Grid>
            </GroupBox>
        </Grid>
        <!-- 하단 상태창 표시-->
        <Grid Grid.Column="0" Grid.ColumnSpan="2" Grid.Row="4">
            <DockPanel>
                <StatusBar x:Name="SB_Main" >
                    <StatusBarItem>
                        <Border>
                            <StackPanel x:Name="DAQ_Panel" Orientation="Horizontal">
                                <iconPacks:PackIconMaterial  Width="12" Foreground="Gray" Kind="ChartLine" Margin="2,2,5,2"/>
                                <TextBlock x:Name="SB_DAQ" Text="DAQ" />
                            </StackPanel>
                        </Border>
                    </StatusBarItem>
                    <StatusBarItem>
                        <Border>
                            <StackPanel Orientation="Horizontal">
                                <iconPacks:PackIconMaterial Width="12" Foreground="Gray" Kind="EngineOutline" Margin="2,2,5,2"/>
                                <TextBlock x:Name="SB_PLC" Text="PLC" />
                            </StackPanel>
                        </Border>
                    </StatusBarItem>
                    <StatusBarItem>
                        <Border>
                            <StackPanel Orientation="Horizontal">
                                <iconPacks:PackIconMaterial  Width="12" Foreground="Gray" Kind="Matrix" Margin="2,2,5,2"/>
                                <TextBlock x:Name="SB_COMPORT" Text="COM1" />
                            </StackPanel>
                        </Border>
                    </StatusBarItem>
                    <StatusBarItem>
                        <TextBlock Width="40" VerticalAlignment="Center"  x:Name="SB_COMSTATE" Text="" />
                    </StatusBarItem>
                    <StatusBarItem>
                        <Border>
                            <StackPanel Orientation="Horizontal">
                                <iconPacks:PackIconMaterial  Width="12" Foreground="Gray" Kind="Security" Margin="2,2,5,2"/>
                                <TextBlock Width="40" VerticalAlignment="Center"  x:Name="SB_CAN" Text="CAN" />
                            </StackPanel>
                        </Border>
                    </StatusBarItem>
                    <Separator Foreground="Yellow" />
                    <StatusBarItem>
                        <TextBlock x:Name="SB_MODE" Text="MANUAL" >
                            <TextBlock.Background>
                                <SolidColorBrush Color="{DynamicResource {x:Static SystemColors.ActiveCaptionColorKey}}"/>
                            </TextBlock.Background>
                        </TextBlock>
                    </StatusBarItem>
                    <Separator Foreground="Yellow" />
                    <StatusBarItem Foreground="Yellow">
                        <TextBlock x:Name="SB_Msg" HorizontalAlignment="Left" VerticalAlignment="Center"/>
                    </StatusBarItem>
                </StatusBar>
            </DockPanel>
        </Grid>
        <!-- 중앙 그래프 표시 및 각종 서브창-->
        <Grid Grid.Column="1" Grid.Row="1" Grid.RowSpan="3" x:Name="MainWorkGrid">
            <TabControl x:Name="MainTab" >
                <!-- No.0 외부 리크 테스트 -->
                <TabItem >
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="*"/>
                            <RowDefinition Height="140"/>
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="10"/>
                        </Grid.ColumnDefinitions>
                        <ni:Graph x:Name="ExtLeakGraph" RenderMode="Raster" Interactions="Zoom, ZoomHorizontal, ZoomVertical, ZoomIn, ZoomOut, Pan, PanHorizontal, PanVertical"
							DataSource="{Binding GraphData}"  BorderThickness="2"  >
                            <ni:Graph.Plots >
                                <ni:Plot x:Name="Ext_P_TestP" AdjustVerticalScale="False" Label="Test Vacuum" VerticalScale="{Binding ElementName=Ext_TestPressure}"  HorizontalScale="{Binding ElementName=Ext_Time}">
                                    <ni:PointPlotRenderer Fill="Green" Shape="Cross" Stroke="Green" Size="2,2" />
                                </ni:Plot>
                                <ni:Plot x:Name="Ext_P_LeakP" AdjustVerticalScale="False" Label="Leak Vacuum" VerticalScale="{Binding ElementName=Ext_LeakPressure}" HorizontalScale="{Binding ElementName=Ext_Time}">
                                    <ni:LinePlotRenderer Stroke="Red" StrokeThickness="4"/>
                                </ni:Plot>
                            </ni:Graph.Plots>
                            <ni:Graph.Axes>
                                <ni:AxisDouble x:Name="Ext_Time"         Range="0.0,20.0"   Orientation="Horizontal" Label="시험시간(초)"  >
                                    <ni:AxisDouble.LabelTemplate>
                                        <DataTemplate>
